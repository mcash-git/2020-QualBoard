{
    "name": "twentytwenty.qualboard.web",
    "version": "4.0.0",
    "description": "QB4 beta",
    "repository": {
        "type": "git",
        "url": "https://github.com/2020IP/TwentyTwenty.QualBoard.Web"
    },
    "main": "dist/main.js",
    "scripts": {
        "build": "npm run build:dev",
        "build:dev": "cross-env NODE_ENV=development BABEL_ENV=dev npm run webpack -- --progress --profile",
        "build:prod": "cross-env NODE_ENV=production BABEL_ENV=prod npm run webpack -- --profile -p",
        "clean": "npm cache clean && del-cli node_modules \"test/coverage\" dist",
        "clean:dist": "del-cli dist",
        "clean:install": "npm install",
        "clean:start": "npm start",
        "design": "npm run server:design",
        "e2e": "concurrently --success first --kill-others \"npm run e2e:start-when-ready\" \"cross-env WEBPACK_PORT=19876 npm start\"",
        "e2e:live": "npm run e2e:start -- --elementExplorer",
        "e2e:start": "cross-env ./node_modules/.bin/protractor test/protractor.conf.js",
        "e2e:start-when-ready": "wait-on --timeout 120000 http-get://localhost:19876/index.html && npm run e2e:start",
        "lint": "eslint src",
        "prebuild:dev": "npm run clean:dist",
        "prebuild:prod": "npm run clean:dist",
        "preclean:install": "npm run clean",
        "preclean:start": "npm run clean",
        "pree2e": "npm run webdriver:update -- --standalone",
        "server": "npm run server:dev",
        "server:design": "cross-env BABEL_ENV=dev node  ARCHTYPE=design dev ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --profile --watch",
        "server:dev": "cross-env NODE_ENV=development BABEL_ENV=dev node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --watch",
        "server:dev:hmr": "npm run server:dev -- --hot",
        "server:prod": "http-server dist --cors",
        "start": "npm run server:dev",
        "storybook": "cross-env BABEL_ENV=dev start-storybook -p 3002 -c .storybook",
        "test": "cross-env BABEL_ENV=test jest --config ./test/unit/jest.config.json",
        "test:watchAll": "jest --watchAll --notify --config ./test/unit/jest.config.json",
        "watch": "npm run watch:dev",
        "watch:dev": "npm run build:dev -- --watch",
        "watch:dev:hmr": "npm run watch:dev -- --hot",
        "watch:prod": "npm run build:prod -- --watch",
        "watch:test": "npm run test -- --auto-watch --no-single-run",
        "webdriver:start": "cross-env ./node_modules/.bin/webdriver-manager start",
        "webdriver:update": "cross-env ./node_modules/.bin/webdriver-manager update",
        "webpack": "cross-env BABEL_ENV=prod node --max-old-space-size=4096 node_modules/.bin/webpack",
        "webpack-dev-server": "cross-env BABEL_ENV=dev ./node_modules/.bin/webpack-dev-server"
    },
